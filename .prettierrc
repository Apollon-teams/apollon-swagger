{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-openapi"],
  "overrides": [
    {
      "files": "*.yaml",
      "options": { "parser": "openapi" }
    },
    {
      "files": "*.yml",
      "options": { "parser": "openapi" }
    },
    {
      "files": "*.json",
      "options": { "parser": "json" }
    }
  ]
}
